{
  "manifest_version": 2,
  "name": "ToitTabs",
  "version": "0.2",
  "description": "Those tabs are so toit...",
  "permissions": [
    "tabs"
  ],
  "browser_action": {
      "default_icon": "TTlogo16.png",
      "default_popup": "index.html"
  },
  "icons": {
          "64": "TTlogo64.png",
          "128": "TTlogo128.png"
  },
  "content_security_policy": "script-src 'self' 'sha256-VH01nMDBrAFhMQl48E+QK22qKcbsiJTkAuk988pEZJs='; object-src 'self'",
  "web_accessible_resources": [
    "index.html",
    "/static/*"
  ]
}
